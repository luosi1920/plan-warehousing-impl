               extends="com.yibai.plan.warehousing.dao.mapper.TestDataMapper.BaseResultMap"></resultMap>


    <sql id="Base_Column_List">
        <include refid="com.yibai.plan.warehousing.dao.mapper.TestDataMapper.Base_Column_List"></include>
    </sql>
    <insert id="saveUser">

    </insert>

    <select id="selectAll" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from test_data a
        order by a.id desc
